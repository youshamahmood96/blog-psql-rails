<h1>Hello</h1>

<%= link_to 'New Post',new_post_path %>

<% @posts.each do |post| %>
  <h2> <%= post.title %> </h2>
  <p> <%= post.body %> </p>
  <p> <%= post.updated_at %> </p>
  <p> <%= post.author.name%> </p>
  <%= link_to 'Edit Post',edit_post_path(post) %>
  <%= link_to 'Delete',post_path(post), method: :delete %>
<% end %>